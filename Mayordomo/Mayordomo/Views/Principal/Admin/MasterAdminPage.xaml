<?xml version="1.0" encoding="UTF-8" ?>
<MasterDetailPage x:Class="Mayordomo.Views.Principal.Admin.MasterAdminPage"
                  xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:admin="clr-namespace:Mayordomo.Views.Principal.Admin"
                  Title="Master"
                  BackgroundColor="White">
    <MasterDetailPage.Master>
        <admin:MenuAdminPage />
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage BarBackgroundColor="Black" BarTextColor="White">
            <x:Arguments>
                <admin:DashboardAdminPage />
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>
